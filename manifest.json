{
    "name": "SutareExtension",
    "short_name": "SutareExtension",
    "version": "0.3.15",
    "manifest_version": 2,
    "description": "古さに応じて表示を汚くする",

    "permissions": [
	"activeTab"
    ],
    
    "applications": {
	"gecko": {
	    "id": "sutare@pitecan.com",
	    "strict_min_version": "45.0"
	}
    },

    "page_action": {
	"default_icon": "icons/sutare-48.png"
    },
    
    "icons": {
	"48": "icons/sutare-48.png",
	"96": "icons/sutare-96.png"
    },

    "content_scripts": [
	{
	    "matches": ["*://*/*"],
	    "js": ["exports.js", "jquery-2.1.4.min.js", "sutare.js"],
	    "run_at": "document_idle"
	}
    ]
}

